#! /bin/sh

case "$1" in
  start)
        su roxanne -c "bash /home/roxanne/roxanne/roxanne.sh start" ; exit	
	;;
  stop)
	su roxanne -c "bash /home/roxanne/roxanne/roxanne.sh stop" ; exit
	;;
  status)
        ;;
  restart|force-reload)
	su roxanne -c "bash /home/roxanne/roxanne/roxanne.sh restart" ; exit
	;;
  *)
	#echo "Usage: $SCRIPTNAME {start|stop|restart|reload|force-reload}" >&2
	echo "Usage: $SCRIPTNAME {start|stop|status|restart|force-reload}" >&2
	exit 3
	;;
